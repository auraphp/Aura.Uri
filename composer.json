{
    "name": "otobank/aura-uri",
    "type": "library",
    "description": "A fork of The Aura Uri package provides objects to build and manipulate URL strings.",
    "keywords": [ "uri", "url" ],
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Aura Contributors",
            "homepage": "https://github.com/auraphp/Aura.Uri/contributors"
        }
    ],
    "require": {
        "php": "^7.2 || ~8.0.0 || ~8.1.0 || ~8.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8 || ^9 || ^10"
    },
    "autoload": {
        "psr-4": {
            "Aura\\Uri\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aura\\Uri\\": "tests/"
        }
    },
    "conflict": {
        "aura/uri": "*"
    }
}
